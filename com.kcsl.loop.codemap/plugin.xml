<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <!-- adds the prioritized codemap stages -->
   <extension point="com.ensoftcorp.open.commons.codemap">
      <codemap class="com.kcsl.loop.codemap.stages.EnhancedLoopCallGraphOverlayCodemapStage"></codemap>
      <codemap class="com.kcsl.loop.codemap.stages.CallSiteOperatorTaintOverlayCodemapStage"></codemap>
      <codemap class="com.kcsl.loop.codemap.stages.ApplicationSpecificConfigurationCodemapStage"></codemap>
      <codemap class="com.kcsl.loop.codemap.stages.LoopCatalogCodemapStage"></codemap>
      <codemap class="com.kcsl.loop.codemap.stages.ReachabilityAnalysisCodemapStage"></codemap>
      <codemap class="com.kcsl.loop.codemap.stages.LambdaCodemapStage"></codemap>
   </extension>

</plugin>